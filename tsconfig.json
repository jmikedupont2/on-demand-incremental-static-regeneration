{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
      "allowJs": true,
      "emitDecoratorMetadata": true,
      "esModuleInterop": true,
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "forceConsistentCasingInFileNames": true,
      "incremental": true,
      "incremental": true,
      "isolatedModules": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "jsx": "preserve",
      "module": "esnext",
      "moduleResolution": "node",
      "noEmit": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "strict": false,
      "plugins": [
	  {
	      "name": "next"
	  }],
      "paths": {	      "@/*": ["./*"]},
      "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "lib/github.ts",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
